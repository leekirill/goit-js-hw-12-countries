<h2 class="country-name">{{name}}</h2>
<div class="profile-wrapper">
    <div class="profile-item">
        <p class="data-item"><b>Capital: </b>{{capital}}</p>
        <p class="data-item"><b>Population: </b>{{population}}</p>
        <ul class="languages-list"><b>Languages:</b>
            {{#each languages}}
            <li class="languages-list__item">{{name}}</li>
            {{/each}}
        </ul>

    </div>
    <div class="profile-item">
        <img src={{flag}} alt=""></div>
</div>